    "scripts": {
        "cp-packages-json": "cp package.json build && cp package-lock.json build",
        "dev-build-init": "mkdirp build && npm run cp-packages-json",
        "dev": "echo // > build/server.js && npm run watch-babel",
        "prestart-dev": "npm run clean && npm run dev-build-init",
        "start-dev": "echo // > build/server.js && npm run babel && npm run babel-packages && concurrently --kill-others \"npm:watch-babel\" \"npm:watch-server\" \"npm:watch-webpack\" \"node cli styles -w\"",
        "start": "node --no-deprecation --unhandled-rejections=strict server.js",
        "predev": "npm run clean && npm run dev-build-init",
        "watch-babel": "cross-env NODE_ENV=development concurrently --kill-others \"npm run babel -- -w\" \"npm run babel-packages -- -w\"",
        "watch-server": "cross-env NODE_ENV=development NODE_OPTIONS=\"--experimental-vm-modules --no-deprecation --unhandled-rejections=strict\" nodemon --legacy-watch -w ./build -w ./packages/**/*.js ./build/server.js",
        "prebuild": "npm run clean && npm run lint && npm run test",
        "build": "cross-env NODE_ENV=node npm run babel && npm run build-packages && npm run build-webpack && node cli styles && npm run cp-packages-json && npm run build-storybook",
        "lint": "cross-env NODE_ENV=test NODE_OPTIONS=\"--no-deprecation --experimental-vm-modules --unhandled-rejections=strict\" eslint -c=.eslintrc 'src/**/*.ts' 'packages/**/*.ts'",
        "test": "cross-env NODE_ENV=test NODE_OPTIONS=\"--no-warnings --no-deprecation --experimental-vm-modules --unhandled-rejections=strict\" jest -c=\"jest.config.ts\" --passWithNoTests",
        "tsc": "tsc --project tsconfig.json --noEmit",
        "tdd": "cross-env NODE_ENV=development npm test -- --watch --coverage=false",
        "tdd-all": "cross-env NODE_ENV=development npm test -- --watchAll --coverage=false",
        "babel": "cross-env NODE_ENV=node babel src -d build --extensions \".ts,.js,.tsx,.jsx\" --copy-files",
        "babel-packages": "cross-env NODE_ENV=node node packages-babel.js",
        "watch-webpack": "rimraf build/client-assets && cross-env NODE_ENV=development npm run webpack -- watch --config webpack.config.ts --stats normal",
        "build-packages": "npm run tsc && npm run babel-packages",
        "build-webpack": "cross-env NODE_ENV=production npm run webpack -- --config webpack.config.ts --stats normal",
        "webpack": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js",
        "clean": "rimraf build && npm run babel-packages -- --clean",
        "cli": "cross-env NODE_ENV=development node --no-deprecation --unhandled-rejections=strict build/cli.js",
        "storybook": "storybook dev -p 8081",
        "build-storybook": "storybook build -o build-storybook"
    },
    "nodemonConfig": {
        "ignore": [
            "**/node_modules/**"
        ],
        "delay": 120,
        "signal": "SIGTERM"
    },
    "workspaces": {
        "packages": [
            "./packages/design-system",
            "./packages/helpers"
        ]
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
